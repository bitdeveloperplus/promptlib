{
  "name": "prompt-manage",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "db:pull": "turbo db:pull",
    "db:generate": "turbo db:generate",
    "lint": "turbo lint",
    "lint:ci": "turbo lint --concurrency=2",
    "check": "turbo check",
    "check:ci": "turbo check:ci",
    "typecheck": "turbo typecheck",
    "typecheck:ci": "turbo typecheck --concurrency=2",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo test",
    "test:ci": "turbo test:ci -- --silent --concurrency=2"
  },
  "devDependencies": {
    "prettier": "3.1.1",
    "turbo": "^2.3.3"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "npm@9.6.7",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "overrides": {
    "zod": "^3.22.4"
  }
}

